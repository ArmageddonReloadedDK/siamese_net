    "dataset='dataset'\n",
    "folders=os.listdir(dataset)\n",
    "N=0\n",
    "for folder in folders:\n",
    "    N+=len(os.listdir(dataset+'/'+str(folder)))\n",
    "pairs=0\n",
    "for folder in folders:\n",
    "    l=len(os.listdir(dataset+'/'+str(folder)))\n",
    "    pairs+=l**2+l*(N-l)"
